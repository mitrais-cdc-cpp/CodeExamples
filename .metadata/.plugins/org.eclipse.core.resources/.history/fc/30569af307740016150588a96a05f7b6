#include <iostream>
#include <string>

#include "../../inc/InheritFromMultipleParents/Vivipar.hpp"

using namespace InheritFromMultipleParents;

Ovipar::Ovipar(int id)
{
	this->id = id;
	this->description = "Oriparous is zoo producing young by means of eggs";
}

std::string Ovipar::getDescription()
{
	return this->description;
}

void Ovipar::makeMovement()
{
	std::cout << "Flying..." << std::endl;
}

void Ovipar::makeSound()
{
	std::cout << "Kwak..kwak.." << std::endl;
}

Ovipar::~Ovipar()
{

}
