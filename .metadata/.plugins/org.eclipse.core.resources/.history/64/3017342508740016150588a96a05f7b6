#include <iostream>
#include <string>

#include "../../inc/InheritFromMultipleParents/Vivipar.hpp"

using namespace InheritFromMultipleParents;

Vivipar::Vivipar(int id)
{
	this->id = id;
	this->description = "Oriparous is zoo producing young by means of eggs";
}

std::string Vivipar::getDescription()
{
	return this->description;
}

void Vivipar::makeMovement()
{
	std::cout << "Flying..." << std::endl;
}

void Vivipar::makeSound()
{
	std::cout << "Aum...Aum..." << std::endl;
}

Vivipar::~Vivipar()
{

}
