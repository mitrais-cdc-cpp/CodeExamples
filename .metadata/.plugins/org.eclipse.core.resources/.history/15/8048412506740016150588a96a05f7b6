#ifndef INC_INHERITFROMMULTIPLEPARENTS_OVIPAR_HPP_
#define INC_INHERITFROMMULTIPLEPARENTS_OVIPAR_HPP_

#include <iostream>
#include <string>

namespace InheritFromMultipleParents
{
	class Ovipar
	{
		public:
			Ovipar(int id);
			virtual ~Ovipar();

			int getId();
			std::string getDescription();
			void makeMovement();
			void makeSound();

		private:
			int id;
			std::string description;
	};
}


#endif /* INC_INHERITFROMMULTIPLEPARENTS_OVIPAR_HPP_ */
