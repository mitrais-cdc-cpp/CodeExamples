#ifndef INC_SCENARIO2_FIREENGINE_HPP_
#define INC_SCENARIO2_FIREENGINE_HPP_

namespace scenario2
{
	class fireEngine
	{
		public:
			fireEngine(int ladderHeight, long fuelCapacity);
			virtual ~fireEngine();

			virtual void showladderHeight();
			virtual void showFuelCapacity();

		protected:
			int ladderHeight;
			long fuelCapacity;

	};
}

#endif /* INC_SCENARIO2_FIREENGINE_HPP_ */
