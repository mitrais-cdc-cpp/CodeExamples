
#ifndef INC_SCENARIO1_FRONTOFFICER_HPP_
#define INC_SCENARIO1_FRONTOFFICER_HPP_

namespace scenario1
{
	class staff
	{
		public:
			staff(std::string staffId, std::time_t joinDate)
			virtual ~staff();
			virtual void showId();
			virtual void showName();

		protected:
			std::string staffId;
			std::time_t joinDate;
	};
}


#endif /* INC_SCENARIO1_FRONTOFFICER_HPP_ */
