#include "../../inc/InheritFromMultipleParents/Ovovivipar.hpp"

using namespace InheritFromMultipleParents;

// Ovipar implementation
Ovovivipar::Ovipar::Ovipar(std::string name)
{
	this->name = name;
	this->description = "Oviparous is a kind of zoo producing young by means of eggs";
}

void Ovovivipar::Ovipar::showName()
{
	std::cout << this->name << std::endl;
}

void Ovovivipar::Ovipar::showDescription()
{
	std::cout << this->description << std::endl;
}

void Ovovivipar::Ovipar::makeMovement()
{
	std::cout << "Flying..." << std::endl;
}

void Ovovivipar::Ovipar::makeSound()
{
	std::cout << "Kwak..kwak.." << std::endl;
}

Ovovivipar::Ovipar::~Ovipar()
{

}
