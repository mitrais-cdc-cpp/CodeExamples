#ifndef INC_SCENARIO1_INDIVIDUAL_HPP_
#define INC_SCENARIO1_INDIVIDUAL_HPP_

#include <string>

namespace scenario1
{
	class individual
	{
		public:
			individual(std::string id, std::string name)
			virtual ~individual();
			virtual void showId();
			virtual void showName();

		protected:
			std::string id;
			std::string name;
	};
}

#endif /* INC_SCENARIO1_INDIVIDUAL_HPP_ */
